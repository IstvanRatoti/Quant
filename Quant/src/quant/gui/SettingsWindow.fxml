<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="ap" fx:controller="quant.gui.SettingsWindowCont" prefHeight="130.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="100.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="100.0" text="Server" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="100.0" text="Username" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="10.0" layoutY="40.0" prefHeight="30.0" prefWidth="100.0" text="Password" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="100.0" prefHeight="100.0" prefWidth="150.0">
         <children>
            <TextField fx:id="server" prefHeight="25.0" prefWidth="145.0">
               <VBox.margin>
                  <Insets right="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="username" layoutX="10.0" layoutY="15.0" prefHeight="25.0" prefWidth="145.0">
               <VBox.margin>
                  <Insets right="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="password" prefHeight="25.0" prefWidth="145.0">
               <VBox.margin>
                  <Insets right="5.0" top="5.0" />
               </VBox.margin>
            </PasswordField>
         </children>
      </VBox>
      <HBox layoutY="95.0" prefHeight="30.0" prefWidth="250.0">
         <children>
            <Button fx:id="okBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0" text="Ok">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Pane prefHeight="25.0" prefWidth="100.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="cancelBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
