<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>



<BorderPane fx:controller="quant.gui.MainWindowCont"
			prefHeight="600.0" prefWidth="400.0"
			xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ScrollPane fx:id="actListScroll" prefHeight="550.0" prefWidth="400.0" BorderPane.alignment="CENTER">
      	<VBox fx:id="actList" />
      </ScrollPane>
   </center>
   <bottom>
      <HBox prefHeight="35.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addBtn" contentDisplay="CENTER" mnemonicParsing="false" text="Add">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Pane prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="optionsBtn" alignment="CENTER" mnemonicParsing="false" text="Options">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
       </HBox>
   </bottom>
</BorderPane>
