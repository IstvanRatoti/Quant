<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox fx:id="baseBox" prefHeight="900.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quant.gui.DetailsWindowCont">
   <children>
      <HBox>
         <children>
            <VBox prefHeight="110.0" prefWidth="150.0">
               <children>
                  <Label alignment="BASELINE_CENTER" prefHeight="30.0" prefWidth="150.0" text="Name">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="150.0" text="Type">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="50.0" prefHeight="30.0" prefWidth="150.0" text="Schedule">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <VBox fx:id="detailsBox" prefHeight="110.0" prefWidth="450.0">
               <children>
                  <TextField fx:id="name" minHeight="30.0" prefWidth="350.0">
                     <VBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
                  <ComboBox fx:id="type" minHeight="30.0" prefWidth="190.0">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </ComboBox>
                  <MenuButton fx:id="newEntryMenuBtn" minHeight="30.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="119.0" text="New entry">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                     <items>
                        <MenuItem fx:id="unscheduled" mnemonicParsing="false" text="Unscheduled" />
                        <MenuItem fx:id="scheduled" mnemonicParsing="false" text="Scheduled" />
                        <MenuItem fx:id="deadline" mnemonicParsing="false" text="Deadline" />
                        <MenuItem fx:id="weekly" mnemonicParsing="false" text="Weekly" />
                        <MenuItem fx:id="daily" mnemonicParsing="false" text="Daily" />
                     </items>
                  </MenuButton>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox fx:id="descBox" layoutY="110.0" prefHeight="750.0" prefWidth="600.0">
         <children>
            <Label fx:id="descLabel" alignment="BASELINE_CENTER" layoutY="-1.0" prefHeight="30.0" prefWidth="150.0" text="Description">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextArea fx:id="desc" layoutX="150.0" prefHeight="750.0" prefWidth="450.0">
               <padding>
                  <Insets bottom="5.0" right="5.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextArea>
         </children>
      </HBox>
      <HBox layoutY="860.0" prefHeight="40.0" prefWidth="600.0">
         <children>
            <Button fx:id="acceptBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="90.0" text="Accept">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="90.0" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
