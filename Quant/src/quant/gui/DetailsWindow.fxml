<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="ap" prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quant.gui.DetailsWindowCont">
   <children>
      <VBox prefHeight="660.0" prefWidth="150.0">
         <children>
            <Label alignment="BASELINE_CENTER" prefHeight="30.0" prefWidth="150.0" text="Name">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="150.0" text="Type">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="40.0" prefHeight="30.0" prefWidth="150.0" text="Date">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="70.0" prefHeight="30.0" prefWidth="150.0" text="Place">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <Label alignment="BASELINE_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="150.0" text="Description">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="150.0" prefHeight="660.0" prefWidth="350.0">
         <children>
            <TextField fx:id="name" prefHeight="30.0" prefWidth="350.0">
               <VBox.margin>
                  <Insets right="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
            <ChoiceBox fx:id="type" prefHeight="30.0" prefWidth="190.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </ChoiceBox>
            <DatePicker fx:id="date" prefHeight="30.0" prefWidth="190.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </DatePicker>
            <TextField fx:id="place" prefHeight="30.0" prefWidth="350.0">
               <VBox.margin>
                  <Insets right="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
            <TextArea fx:id="desc" prefHeight="500.0" prefWidth="345.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </TextArea>
         </children>
      </VBox>
      <HBox layoutY="660.0" prefHeight="40.0" prefWidth="500.0">
         <children>
            <Button fx:id="acceptBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="90.0" text="Accept">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" prefHeight="30.0" prefWidth="90.0" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
